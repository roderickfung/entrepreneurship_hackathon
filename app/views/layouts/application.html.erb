<!DOCTYPE html>
<html>
  <head>
    <title>Hackathon</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- Map Script -->
    <script src="//maps.google.com/maps/api/js?key=AIzaSyCy1NBsfYySEaaNN8c64ZUa66hK4NJSuaE"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->

  </head>

  <body>
    <!-- NavBar -->
    <%if user_signed_in? %>
      <%= render partial: '/layouts/partials/user_nav'%>
    <%else%>
      <%= render partial: '/layouts/partials/top_nav'%>
    <%end%>

    <!-- Flash Messages -->
    <div class='container'>
      <div style='margin-top: 100px;'></div>
      <div class='alert-success' style='z-index: 1;'>
        <center><%=flash[:notice]%></center>
      </div>
      <div class='alert-warning' style='z-index: 1;'>
        <center><%=flash[:alert]%></center>
      </div>
    </div>

    <!-- Content -->
    <%= yield %>

    <!-- Footer -->
    <div class='footer'>
      <div class='container'>
        <%= render '/layouts/partials/footer2'%>
      </div>
    </div>

    <%= render partial: '/modals/signin' %>
    <%= render partial: '/modals/register', participant: @participant %>

    <!-- Keynote List -->
    <%= render partial: '/speakers/temp_partials/danieldubois'%>
    <%= render partial: '/speakers/temp_partials/igorfaletski'%>
    <!-- Speaker List -->
    <%= render partial: '/speakers/temp_partials/amyngai2'%>
    <%= render partial: '/speakers/temp_partials/manishanarula'%>
    <%= render partial: '/speakers/temp_partials/husseinhallak'%>
    <%= render partial: '/speakers/temp_partials/chinghingchang'%>
    <%= render partial: '/speakers/temp_partials/keithippel'%>
    <%= render partial: '/speakers/temp_partials/loriecorcuera'%>
    <%= render partial: '/speakers/temp_partials/amyngai'%>
    <%= render partial: '/speakers/temp_partials/paulinacameron'%>
    <%= render partial: '/speakers/temp_partials/rogerhaskett'%>
    <%= render partial: '/speakers/temp_partials/miluntesovic'%>
    <%= render partial: '/speakers/temp_partials/willsoncross'%>
    <%= render partial: '/speakers/temp_partials/jonathannatavio'%>
  </body>
</html>
